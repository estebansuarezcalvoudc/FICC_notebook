    "<figure><center>\n",
    "<a id='fig_ref'></a>\n",
    "<img src=\"images/Hadamard_measureb.png\" align=center width='50%'/>\n",
    "<center></center>\n",
    "</center></figure>\n",
    "<br>\n"
   ]
  },
